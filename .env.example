# OpenRouter API key
OPENROUTER_API_KEY=your_key_here

# Enable SQL by default (optional)
ENABLE_SQL=false

# Optional SQLite DB path
SOURCE_DB_PATH=data/source.db

# Postgres DB URL
DATABASE_URL=postgresql+psycopg://pdf_user:pdf_pass@localhost:5432/pdf_converter


# OpenRouter base URL override (optional)
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Log full LLM request/response payloads (optional)
LOG_LLM_PAYLOAD=false
